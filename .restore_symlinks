#! /bin/bash
if [ ! -f ./filters_misc/modes-3.3.mf ]; then ln -s ../nonppr/misc/modes-3.3.mf ./filters_misc/modes-3.3.mf || exit 1; fi
if [ ! -f ./include/gu_md5.h ]; then ln -s ../nonppr_misc/md5/gu_md5.h ./include/gu_md5.h || exit 1; fi
if [ ! -f ./include/cexcept.h ]; then ln -s ../nonppr_misc/cexcept-1.0.0/cexcept.h ./include/cexcept.h || exit 1; fi
if [ ! -f ./libgu/gu_md5.c ]; then ln -s ../nonppr_misc/md5/gu_md5.c ./libgu/gu_md5.c || exit 1; fi
if [ ! -f ./libgu/snprintf.c ]; then ln -s ../nonppr_misc/misc/snprintf.c ./libgu/snprintf.c || exit 1; fi
if [ ! -f ./libgu/gu_wildmat.c ]; then ln -s ../nonppr_misc/misc/gu_wildmat.c ./libgu/gu_wildmat.c || exit 1; fi
if [ ! -f ./libscript/MD5pp.pm ]; then ln -s ../nonppr_misc/md5/MD5pp.pm ./libscript/MD5pp.pm || exit 1; fi
if [ ! -f ./libttf/ps_type3.c ]; then ln -s ../nonppr_misc/misc/ps_type3.c ./libttf/ps_type3.c || exit 1; fi
if [ ! -f ./makeprogs/squeeze.c ]; then ln -s ../nonppr_misc/misc/squeeze.c ./makeprogs/squeeze.c || exit 1; fi
if [ ! -f ./misc/README.txt ]; then ln -s ../docs/misc/misc_dir_contents.txt ./misc/README.txt || exit 1; fi
if [ ! -f ./po/README.txt ]; then ln -s ../docs/misc/internationalization.txt ./po/README.txt || exit 1; fi
if [ ! -f ./pprpopup/md5pure.tcl ]; then ln -s ../nonppr_misc/md5/md5pure.tcl ./pprpopup/md5pure.tcl || exit 1; fi
if [ ! -f ./www/md5.js ]; then ln -s ../nonppr_misc/md5/md5.js ./www/md5.js || exit 1; fi
if [ ! -f ./History.txt ]; then ln -s docs/misc/History.txt ./History.txt || exit 1; fi
if [ ! -f ./PORTING.txt ]; then ln -s docs/misc/PORTING.txt ./PORTING.txt || exit 1; fi
if [ ! -f ./TODO.txt ]; then ln -s docs/misc/TODO.txt ./TODO.txt || exit 1; fi
if [ ! -f ./LICENSE.txt ]; then ln -s docs/misc/LICENSE.txt ./LICENSE.txt || exit 1; fi
if [ ! -f ./INSTALL.txt ]; then ln -s docs/misc/INSTALL.txt ./INSTALL.txt || exit 1; fi
