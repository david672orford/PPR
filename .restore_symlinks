#! /bin/bash
# ../../INSTALL.txt --> ./docs/misc/INSTALL.txt
if [ ! -f ./docs/misc/INSTALL.txt ]; then ln -sf ../../INSTALL.txt ./docs/misc/INSTALL.txt || exit 1; fi
# ../../History.txt --> ./docs/misc/main_history.txt
if [ ! -f ./docs/misc/main_history.txt ]; then ln -sf ../../History.txt ./docs/misc/main_history.txt || exit 1; fi
# ../../po/README.txt --> ./docs/misc/internationalization.txt
if [ ! -f ./docs/misc/internationalization.txt ]; then ln -sf ../../po/README.txt ./docs/misc/internationalization.txt || exit 1; fi
# ../History.txt --> ./docs/misc/docs_history.txt
if [ ! -f ./docs/misc/docs_history.txt ]; then ln -sf ../History.txt ./docs/misc/docs_history.txt || exit 1; fi
# ../../misc/README.txt --> ./docs/misc/misc_dir_contents.txt
if [ ! -f ./docs/misc/misc_dir_contents.txt ]; then ln -sf ../../misc/README.txt ./docs/misc/misc_dir_contents.txt || exit 1; fi
# ../nonppr_misc/md5/gu_md5.h --> ./include/gu_md5.h
if [ ! -f ./include/gu_md5.h ]; then ln -sf ../nonppr_misc/md5/gu_md5.h ./include/gu_md5.h || exit 1; fi
# ../nonppr_misc/cexcept-1.0.0/cexcept.h --> ./include/cexcept.h
if [ ! -f ./include/cexcept.h ]; then ln -sf ../nonppr_misc/cexcept-1.0.0/cexcept.h ./include/cexcept.h || exit 1; fi
# ../nonppr_misc/pcre/pcre.h --> ./include/pcre.h
if [ ! -f ./include/pcre.h ]; then ln -sf ../nonppr_misc/pcre/pcre.h ./include/pcre.h || exit 1; fi
# ../nonppr_misc/c2lib/hash.h --> ./include/hash.h
if [ ! -f ./include/hash.h ]; then ln -sf ../nonppr_misc/c2lib/hash.h ./include/hash.h || exit 1; fi
# ../nonppr_misc/c2lib/pool.h --> ./include/pool.h
if [ ! -f ./include/pool.h ]; then ln -sf ../nonppr_misc/c2lib/pool.h ./include/pool.h || exit 1; fi
# ../nonppr_misc/c2lib/pre.h --> ./include/pre.h
if [ ! -f ./include/pre.h ]; then ln -sf ../nonppr_misc/c2lib/pre.h ./include/pre.h || exit 1; fi
# ../nonppr_misc/c2lib/pstring.h --> ./include/pstring.h
if [ ! -f ./include/pstring.h ]; then ln -sf ../nonppr_misc/c2lib/pstring.h ./include/pstring.h || exit 1; fi
# ../nonppr_misc/c2lib/tree.h --> ./include/tree.h
if [ ! -f ./include/tree.h ]; then ln -sf ../nonppr_misc/c2lib/tree.h ./include/tree.h || exit 1; fi
# ../nonppr_misc/c2lib/vector.h --> ./include/vector.h
if [ ! -f ./include/vector.h ]; then ln -sf ../nonppr_misc/c2lib/vector.h ./include/vector.h || exit 1; fi
# ../nonppr_misc/md5/MD5pp.pm --> ./libscript/MD5pp.pm
if [ ! -f ./libscript/MD5pp.pm ]; then ln -sf ../nonppr_misc/md5/MD5pp.pm ./libscript/MD5pp.pm || exit 1; fi
# docbook-xml-4.2/ --> ./nonppr_misc/docbook-xml-4.2/docbook-xml-4.2
if [ ! -f ./nonppr_misc/docbook-xml-4.2/docbook-xml-4.2 ]; then ln -sf docbook-xml-4.2/ ./nonppr_misc/docbook-xml-4.2/docbook-xml-4.2 || exit 1; fi
# docbook-xsl-1.60.1 --> ./nonppr_misc/docbook-xsl-1.60.1/docbook-xsl-1.60.1
if [ ! -f ./nonppr_misc/docbook-xsl-1.60.1/docbook-xsl-1.60.1 ]; then ln -sf docbook-xsl-1.60.1 ./nonppr_misc/docbook-xsl-1.60.1/docbook-xsl-1.60.1 || exit 1; fi
# docbook-xml-4.2/ --> ./nonppr_misc/docbook-xml
if [ ! -f ./nonppr_misc/docbook-xml ]; then ln -sf docbook-xml-4.2/ ./nonppr_misc/docbook-xml || exit 1; fi
# docbook-xsl-1.60.1 --> ./nonppr_misc/docbook-xsl
if [ ! -f ./nonppr_misc/docbook-xsl ]; then ln -sf docbook-xsl-1.60.1 ./nonppr_misc/docbook-xsl || exit 1; fi
# ../nonppr_misc/md5/md5pure.tcl --> ./pprpopup/md5pure.tcl
if [ ! -f ./pprpopup/md5pure.tcl ]; then ln -sf ../nonppr_misc/md5/md5pure.tcl ./pprpopup/md5pure.tcl || exit 1; fi
# ../nonppr_misc/md5/md5.js --> ./www/md5.js
if [ ! -f ./www/md5.js ]; then ln -sf ../nonppr_misc/md5/md5.js ./www/md5.js || exit 1; fi
