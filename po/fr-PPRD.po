#: ../pprd/pprd_ppop.c:963
#, c-format
msgid "\"%s\" does not have a bin called \"%s\".\n"
msgstr "\"%s\" n'a pas de bac au nom de \"%s\".\n"

#: ../pprd/pprd_ppop.c:377
#, c-format
msgid "\"%s\" is a group of printers, not a printer.\n"
msgstr "\"%s\" est un groupe d'imprimantes, pas une imprimante.\n"

#: ../pprd/pprd_ppop.c:951
#, c-format
msgid "\"%s\" is not a printer.\n"
msgstr "\"%s\" n'est pas une imprimante.\n"

#: ../pprd/pprd_ppop.c:1027 ../pprd/pprd_ppop.c:1113
#, c-format
msgid "\"%s\" is not a valid destination.\n"
msgstr "\"%s\" n'est pas une destination valide.\n"

#: ../pprd/pprd_ppop.c:1342
#, c-format
msgid "%d files were not moved because they were being printed.\n"
msgstr ""
"%d fichiers n'ont pas été déplacés car ils étaient en cours d'impression.\n"

#: ../pprd/pprd_ppop.c:1333
#, c-format
msgid "%d files where moved.\n"
msgstr "%d fichiers ont été déplacés.\n"

#: ../pprd/pprd_mainsup.c:296
#, c-format
msgid "%s: Only \"%s\" or \"root\" may start %s.\n"
msgstr "%s : seuls \"%s\" et \"root\" peuvent démarrer %s.\n"

#: ../pprd/pprd_mainsup.c:252
#, c-format
msgid ""
"%s: Security problem: egid = %u\n"
"(This program should be setgid \"%s\".)\n"
msgstr ""
"%s : problème de sécurité : egid = %u\n"
"(Ce programme devrait être setgid \"%s\".)\n"

#: ../pprd/pprd_mainsup.c:244
#, c-format
msgid ""
"%s: Security problem: euid = %u\n"
"(This program should be setuid \"%s\".)\n"
msgstr ""
"%s : problème de sécurité : euid = %u\n"
"(Ce programme devrait être setuid \"%s\".)\n"

#: ../pprd/pprd_mainsup.c:225
#, c-format
msgid "%s: The group \"%s\" doesn't exist.\n"
msgstr "%s : le groupe \"%s\" n'existe pas.\n"

#: ../pprd/pprd_mainsup.c:219
#, c-format
msgid "%s: The user \"%s\" doesn't exist.\n"
msgstr "%s : l'utilisateur \"%s\" n'existe pas.\n"

#: ../pprd/pprd_mainsup.c:302
#, c-format
msgid ""
"%s: User \"%s\" may only start %s if group is \"%s\".\n"
"(Change 4th field in /etc/passwd for user \"%s\".)\n"
msgstr ""
"%s : l'utilisateur \"%s\" ne peut démarrer %s que si le groupe est \"%s\".\n"
"(Modifiez le 4ème champ de /etc/passwd pour l'utilisateur \"%s\".)\n"

#: ../pprd/pprd_mainsup.c:233
#, c-format
msgid "%s: Warning: primary group for user \"%s\" is not \"%s\".\n"
msgstr ""
"%s : avertissement : le groupe primaire de l'utilisateur \"%s\" n'est pas "
"\"%s\".\n"

#: ../pprd/pprd_ppop.c:1329
msgid "1 file was moved.\n"
msgstr "1 fichier a été déplacé.\n"

#: ../pprd/pprd_ppop.c:1340
msgid "1 file was not moved because it was being printed.\n"
msgstr "1 fichier n'a pas été déplacé car il était en cours d'impression.\n"

# msgid "Notify feature already in use for \"%s\".\n"
# msgstr "Possibilité d'annonce déjà utilisée pour \"%s\".\n"
#: ../pprd/pprd_ppop.c:504
#, c-format
msgid ""
"Another process is already using the notify\n"
"feature for the printer \"%s\".\n"
msgstr ""
"Un autre processus utilise déjà la fonction\n"
"d'annonce pour l'imprimante \"%s\".\n"

# msgid "Converting cancel to hold for job \"%s\".\n"
# msgstr "Conversion d'annulation vers suspension pour le travail \"%s\".\n"
#: ../pprd/pprd_ppop.c:600
#, c-format
msgid ""
"Converting outstanding cancel order for\n"
"job \"%s\" to a hold order.\n"
msgstr ""
"Conversion de l'ordre d'annulation en attente pour le\n"
"travail \"%s\" en un ordre de suspension.\n"

#: ../pprd/pprd_ppop.c:111
#, c-format
msgid "Destination \"%s\" does not exist.\n"
msgstr "La destination \"%s\" n'existe pas.\n"

# msgid "\"%s\" has unknown status %d.\n"
# msgstr "\"%s\" a un état %d inconnu.\n"
#: ../pprd/pprd_ppop.c:634
#, c-format
msgid "Internal pprd error: job \"%s\" has unknown status %d.\n"
msgstr "Erreur interne de pprd : le travail \"%s\" a un état %d inconnu.\n"

# msgid "spooler error, \"%s\" is in an undefined state.\n"
# msgstr "erreur de gestionnaire de file, \"%s\" est dans un état indéfini.\n"
#: ../pprd/pprd_ppop.c:427 ../pprd/pprd_ppop.c:492
#, c-format
msgid "Internal pprd error: printer \"%s\" is in an undefined state.\n"
msgstr ""
"Erreur interne de pprd : l'imprimante \"%s\" est dans un état indéfini.\n"

#: ../pprd/pprd_ppop.c:1323
#, c-format
msgid "Job \"%s\" does not exist.\n"
msgstr "Le travail \"%s\" n'existe pas.\n"

#: ../pprd/pprd_ppop.c:1316
#, c-format
msgid "No jobs are queued for \"%s\".\n"
msgstr "Aucun travail n'est en attente pour \"%s\".\n"

#: ../pprd/pprd_ppop.c:1159 ../pprd/pprd_ppop.c:1168 ../pprd/pprd_ppop.c:1380
#, c-format
msgid "No printer or group is called \"%s\".\n"
msgstr "Aucune imprimante ni groupe ne s'appelle \"%s\".\n"

#: ../pprd/pprd_ppop.c:944
#, c-format
msgid "Printer \"%s\" does not exist.\n"
msgstr "L'imprimante \"%s\" n'existe pas.\n"

#: ../pprd/pprd_pprdrv.c:421
msgid "Printer placed in auto-retry mode."
msgstr "Imprimante placée en mode de tentative automatique."

#: ../pprd/pprd_pprdrv.c:415
msgid "Printer placed in fault mode, no auto-retry."
msgstr "Imprimante placée en mode de panne, pas de tentative automatique."

#: ../pprd/pprd_ppop.c:1416
#, c-format
msgid "Queue entry \"%s\" does not exist.\n"
msgstr "L'entrée de file \"%s\" n'existe pas.\n"

#: ../pprd/pprd_remind.c:107
msgid "Remaining printer problems"
msgstr "Problèmes subsistant sur l'imprimante"

#: ../pprd/pprd_ppop.c:615
#, c-format
msgid "Seizing job \"%s\" which is printing on \"%s\".\n"
msgstr "Saisie du travail \"%s\" qui est en cours d'impression sur \"%s\".\n"

#: ../pprd/pprd_ppop.c:307 ../pprd/pprd_ppop.c:554 ../pprd/pprd_ppop.c:720
#: ../pprd/pprd_ppop.c:874
#, c-format
msgid "The destination \"%s\" does not exist.\n"
msgstr "La destination \"%s\" n'existe pas.\n"

#: ../pprd/pprd_ppop.c:659
#, c-format
msgid ""
"The print job \"%s\" can't be released until an\n"
"outstanding hold order has been fully executed.\n"
msgstr ""
"Le travail d'impression \"%s\" ne peut être libéré avant\n"
"que l'opération de suspension en cours ne soit complètement terminée.\n"

# "\"%s\" ne peut être libéré avant que l'opération de suspension\n"
# "soit terminée.\n"
#: ../pprd/pprd_ppop.c:682 ../pprd/pprd_ppop.c:829
#, c-format
msgid "The print job \"%s\" does not exist.\n"
msgstr "Le travail d'impression \"%s\" n'existe pas.\n"

# msgid "\"%s\" is already held.\n"
# msgstr "\"%s\" est déjà suspendu.\n"
#: ../pprd/pprd_ppop.c:584
#, c-format
msgid "The print job \"%s\" is already held.\n"
msgstr "Le travail d'impression \"%s\" est déjà suspendu.\n"

# msgid "\"%s\" is already in transition to the held state.\n"
# msgstr "\"%s\" est déjà en cours vers l'état suspendu.\n"
#: ../pprd/pprd_ppop.c:593
#, c-format
msgid ""
"The print job \"%s\" is already undergoing a\n"
"transition to the held state.\n"
msgstr ""
"Le travail d'impression \"%s\" est déjà en cours de\n"
"transition vers l'état suspendu.\n"

# msgid "\"%s\" is arrested.\n"
# msgstr "\"%s\" est arrêté.\n"
#: ../pprd/pprd_ppop.c:588
#, c-format
msgid "The print job \"%s\" is arrested.\n"
msgstr "Le travail d'impression \"%s\" est arrêté.\n"

# msgid "\"%s\" is not held.\n"
# msgstr "\"%s\" n'est pas suspendu.\n"
#: ../pprd/pprd_ppop.c:668
#, c-format
msgid "The print job \"%s\" is not being held.\n"
msgstr "Le travail d'impression \"%s\" n'est pas suspendu.\n"

#: ../pprd/pprd_ppop.c:370
#, c-format
msgid "The printer \"%s\" does not exist.\n"
msgstr "L'imprimante \"%s\" n'existe pas.\n"

#: ../pprd/pprd_remind.c:144
#, c-format
msgid ""
"The printer \"%s\" has been otherwise engaged or off line for %d minutes.\n"
msgstr "L'imprimante \"%s\" est occupée ou hors ligne depuis %d minutes.\n"

# msgid "\"%s\" is already halting.\n"
# msgstr "\"%s\" est déjà en train de s'arrêter.\n"
#: ../pprd/pprd_remind.c:125
#, c-format
msgid "The printer \"%s\" has suffered %d faults.\n"
msgstr "L'imprimante \"%s\" a subi %d pannes.\n"

#: ../pprd/pprd_remind.c:131
#, c-format
msgid ""
"The printer \"%s\" has suffered a fault from\n"
"which it can not recover on its own.\n"
msgstr ""
"L'imprimante \"%s\" a subi une panne qu'elle\n"
"ne peut résoudre toute seule.\n"

# msgid "\"%s\" is already halting.\n"
# msgstr "\"%s\" est déjà en train de s'arrêter.\n"
#: ../pprd/pprd_ppop.c:446
#, c-format
msgid "The printer \"%s\" is already halting.\n"
msgstr "L'imprimante \"%s\" est déjà en train de s'arrêter.\n"

# msgid "\"%s\" is already printing.\n"
# msgstr "\"%s\" est déjà en cours d'impression.\n"
#: ../pprd/pprd_ppop.c:398
#, c-format
msgid "The printer \"%s\" is already printing.\n"
msgstr "L'imprimante \"%s\" est déjà en train d'imprimer.\n"

# msgid "\"%s\" is already stopping.\n"
# msgstr "\"%s\" est déjà en cours d'arrêt.\n"
#: ../pprd/pprd_ppop.c:465
#, c-format
msgid "The printer \"%s\" is already stopping.\n"
msgstr "L'imprimante \"%s\" est déjà en train de s'arrêter.\n"

# msgid "\"%s\" is already stopt.\n"
# msgstr "\"%s\" est déjà arrêté.\n"
#: ../pprd/pprd_ppop.c:451
#, c-format
msgid "The printer \"%s\" is already stopt.\n"
msgstr "L'imprimante \"%s\" est déjà arrêtée.\n"

# msgid "\"%s\" is not stopt.\n"
# msgstr "\"%s\" n'est pas arrêté.\n"
#: ../pprd/pprd_ppop.c:393
#, c-format
msgid "The printer \"%s\" is not stopt.\n"
msgstr "L'imprimante \"%s\" n'est pas arrêtée.\n"

# msgid "There is an outstanding halt command for \"%s\".\n"
# msgstr "Il y a une commande d'arrêt en suspens pour \"%s\".\n"
#: ../pprd/pprd_ppop.c:411
#, c-format
msgid "There is an outstanding halt command for the printer \"%s\".\n"
msgstr "Il y a une commande d'arrêt en cours pour l'imprimante \"%s\".\n"

#: ../pprd/pprd_respond.c:255
#, c-format
msgid "Undefined response code %d for your job \"%s\"."
msgstr "Code de réponse %d indéfini pour votre travail \"%s\"."

#: ../pprd/pprd_respond.c:221
#, c-format
msgid "Your print job \"%s\" has been canceled."
msgstr "Votre travail d'impression \"%s\" a été annulé."

#: ../pprd/pprd_respond.c:210
#, c-format
msgid "Your print job \"%s\" has been printed on \"%s\"."
msgstr "Votre travail d'impression \"%s\" a été imprimé sur \"%s\"."

#: ../pprd/pprd_respond.c:243
#, c-format
msgid ""
"Your print job \"%s\" is stranded because\n"
"the printer \"%s\" is incapable of printing it."
msgstr ""
"Votre travail d'impression \"%s\" a été arrêté parce que\n"
"l'imprimante \"%s\" est incapable de l'imprimer."

#: ../pprd/pprd_respond.c:249
#, c-format
msgid ""
"Your print job \"%s\" is stranded because no\n"
"member of the group \"%s\" is capable of printing it."
msgstr ""
"Votre travail d'impression \"%s\" a été arrêté parce qu'aucun\n"
"membre du groupe \"%s\" n'est capable de l'imprimer."

#: ../pprd/pprd_respond.c:215
#, c-format
msgid ""
"Your print job \"%s\" was arrested after an attempt\n"
"to print it on \"%s\" resulted in a job error."
msgstr ""
"Votre travail d'impression \"%s\" a été arrêté après qu'une tentative\n"
"de l'imprimer sur \"%s\" ait donné une erreur."

#: ../pprd/pprd_respond.c:231
#, c-format
msgid ""
"Your print job \"%s\" was canceled because\n"
"\"%s\" is not a known destination."
msgstr ""
"Votre travail d'impression \"%s\" a été annulé parce que\n"
"\"%s\" n'est pas une destination connue."

#: ../pprd/pprd_respond.c:237
#, c-format
msgid ""
"Your print job \"%s\" was canceled because\n"
"the destination \"%s\" is not acceping requests."
msgstr ""
"Votre travail d'impression \"%s\" a été annulé parce que\n"
"la destination \"%s\" n'accepte pas les requêtes."

#: ../pprd/pprd_respond.c:226
#, c-format
msgid "Your print job \"%s\" was canceled while printing on \"%s\"."
msgstr ""
"Votre travail d'impression \"%s\" a été annulé pendant l'impression sur "
"\"%s\"."
