#: ../ppr/ppr_main.c:895
msgid "\t--charge-to <string>       PPR charge account to bill for job\n"
msgstr ""
"\t--charge-to <chaîne>       compte PPR sur lequel imputer le travail\n"

#: ../ppr/ppr_main.c:913
msgid ""
"\t--commentary <n>           send commentary messages of types <n>\n"
"\t--commentator <method>     use commentator program <method>\n"
"\t--commentator-address <address>\n"
"\t                           address for commentator program\n"
"\t--commentator-options <options>\n"
"\t                           option list for commentator program\n"
msgstr ""
"\t--commentary <n>        envoie des commentaires de type <n>\n"
"\t--commentator <méthode> utilise le programme <méthode> pour commenter\n"
"\t--commentator-address <adresse>\n"
"\t                        adresse du programme de commentaires\n"
"\t--commentator-options <options>\n"
"\t                        liste d'options pour le programme de commentaires\n"

#: ../samba/ppr2samba.c:577
msgid "\t--debug      explain what is being done\n"
msgstr "\t--debug      explique ce qui se passe\n"

#: ../ppr/ppr_main.c:1062
msgid "\t--editps-level <pos int>   set level of editing for -H editps\n"
msgstr ""
"\t--editps-level <entier positif>   donne le niveau d'édition pour\n"
"\t                                  l'option -H editps\n"

#: ../samba/ppr2samba.c:580
msgid "\t--help       print this message\n"
msgstr "\t--help       affiche ce message\n"

#: ../ppr/ppr_main.c:1042
msgid "\t--hold                     job should be held immediately\n"
msgstr "\t --hold                   travail à retenir immédiatement\n"

#: ../ppr/ppr_main.c:1054
msgid "\t--lpq-filename <string>    filename for ppop lpq listings\n"
msgstr ""
"\t--lpq-filename <chaîne>   nom de fichier pour les affichages de ppop lpq\n"

#: ../samba/ppr2samba.c:578
#, c-format
msgid "\t--nocreate   don't create %s if it doesn't exist\n"
msgstr "\t--nocreate   ne crée pas %s s'il n'existe pas\n"

#: ../ppr/ppr_main.c:1035
msgid "\t--page-list <page-list>    print only the pages indicated\n"
msgstr "\t--page-list <liste-de-pages>\tn'imprime que les pages indiquées\n"

#: ../ppr/ppr_main.c:904
msgid "\t--routing <string>         set DSC routing instructions\n"
msgstr ""
"\t--routing <chaîne>           indique les instructions de routage DSC\n"

#: ../ppr/ppr_main.c:1065
msgid ""
"\t--show-jobid               print the queue id of submitted jobs\n"
"\t--show-long-jobid          print queue id in long format\n"
msgstr ""
"\t--show-jobid               affiche le numéro dans la queue des travaux "
"soumis\n"
"\t--show-long-jobid          affiche le numéro dans la queue au format long\n"

#: ../ppr/ppr_main.c:937
msgid ""
"\t--strip-cache true         strip cached resources from job file\n"
"\t--strip-cache false        don't strip cached resources (default)\n"
msgstr ""
"\t--strip-cache true    enlève les ressources en cache du fichier de "
"travail\n"
"\t--strip-cache false   n'enlève pas les ressources en cache (défaut)\n"

#: ../ppr/ppr_main.c:941
msgid ""
"\t--strip-fontindex true     strip fonts in font index from job file\n"
"\t--strip-fontindex false    don't strip fonts in font index (defualt)\n"
msgstr ""
"\t--strip-fontindex true     enlever les fontes de l'index du fichier de "
"travail\n"
"\t--strip-fontindex false    ne pas enlever les fontes de l'index (défaut)\n"

#: ../ppr/ppr_main.c:945
msgid ""
"\t--strip-printer true       strip out resources already in printer\n"
"\t--strip-printer false      don't strip printer resources (default)\n"
msgstr ""
"\t--strip-printer true     enlève les ressources déjà présentes dans "
"l'imprimante\n"
"\t--strip-printer false    ne pas enlever les ressources de l'imprimante "
"(défaut)\n"

#: ../ppr/ppr_main.c:898
msgid "\t--title <string>           set default document title\n"
msgstr "\t--title <chaîne>        positionne le titre du document par défaut\n"

#: ../fontutils/ppr-indexfonts.c:422 ../ppop/ppop.c:1096
msgid ""
"\t--version\n"
"\t--help\n"
msgstr ""
"\t--version\n"
"\t--help\n"

#: ../ppr/ppr_main.c:1069
msgid ""
"\t--version                  print PPR version information\n"
"\t--help                     print this help\n"
msgstr ""
"\t--version                  affiche les informations sur la version de PPR\n"
"\t--help                     affiche cette aide\n"

#: ../samba/ppr2samba.c:579
msgid "\t--version    print the PPR version number\n"
msgstr "\t--version    affiche le numéro de version de PPR\n"

#: ../ppr/ppr_main.c:934
msgid "\t-A                         root or ppr has preauthorized\n"
msgstr ""
"\t-A                        root ou ppr a fait l'autorisation préalable\n"

#: ../lprsrv/lprsrv.c:166
msgid ""
"\t-A <seconds>\n"
"\t--arrest-interest-interval <seconds>\n"
"\t\t(this switch is passed thru to ppop)\n"
"\t--version\n"
"\t\t(print PPR version number)\n"
"\t--help\n"
"\t\t(print this message)\n"
msgstr ""
"\t-A <secondes>\n"
"\t--arrest-interest-interval <secondes>\n"
"\t\t(cette bascule est passée directement à ppop)\n"
"\t--version\n"
"\t\t(affiche le numéro de version de PPR)\n"
"\t--help\n"
"\t\t(affiche ce message)\n"

#: ../ppr/ppr_main.c:968
msgid ""
"\t-B false                   disable automatic bin selection\n"
"\t-B true                    enable automatic bin selection (default)\n"
msgstr ""
"\t-B false                   désactive la sélection automatique du bac\n"
"\t-B true                    permet la sélection automatique du bac "
"(défaut)\n"

#: ../ppr/ppr_main.c:965
msgid "\t-D <mediumname>            sets default medium\n"
msgstr "\t-D <nom de support>       positionne le support par défaut\n"

#: ../ppr/ppr_main.c:958
msgid "\t-F '<feature name>'        inserts setup code for a printer feature\n"
msgstr ""
"\t-F '<nom de possibilité>'   insère le code d'initialisation\n"
"\t                            d'une possibilité d'imprimante\n"

#: ../ppr/ppr_main.c:1038
msgid ""
"\t-G <string>                gab about subject indicated by <string>\n"
"\t--gab <string>             same as -G\n"
msgstr ""
"\t-G <chaîne>                parle du sujet indiqué par <chaîne>\n"
"\t--gab <chaîne>             pareil que -G\n"

#: ../ppr/ppr_main.c:1057
msgid ""
"\t-H <hack name>             turn on a hack\n"
"\t-H no-<hack name>          turn off a hack\n"
"\t--hack [no-]<hack name>    same as -H\n"
msgstr ""
"\t-H <nom de bitouille>      lance une bitouille\n"
"\t-H no-<nom de bitouille>   arrête une bitouille\n"
"\t--hack [no-]<nom de bitouille>    identique à -H\n"

#: ../ppr/ppr_main.c:961
msgid ""
"\t-K true                    keep feature code though not in PPD file\n"
"\t-K false                   don't keep bad feature code (default)\n"
msgstr ""
"\t-K true                 garde le code de la possibilité même si elle\n"
"\t                        ne se trouve pas dans le fichier PPD\n"
"\t-K false                ne garde pas le mauvais code (défaut)\n"

#: ../ppr/ppr_main.c:979
msgid ""
"\t-N <positive integer>      print pages N-Up\n"
"\t-N noborders               turn off N-Up borders\n"
msgstr ""
"\t-N <entier positif>        imprime N pages par feuille\n"
"\t-N noborders               désactive les bordures avec N-pages\n"

#: ../ppr/ppr_main.c:901
msgid "\t-O <string>                overlay `Draft' notice\n"
msgstr ""
"\t-O <chaîne>               indication de `Brouillon' par-dessus le texte\n"

#: ../ppr/ppr_main.c:1014
msgid ""
"\t-P notifyme                set ProofMode to NotifyMe\n"
"\t-P substitute              set ProofMode to Substitute\n"
"\t-P trustme                 set ProofMode to TrustMe\n"
msgstr ""
"\t-P notifyme                positionne ProofMode à NotifyMe\n"
"\t-P substitute              positionne ProofMode à Substitute\n"
"\t-P trustme                 positionne ProofMode à TrustMe\n"

#: ../ppr/ppr_main.c:1051
msgid "\t-Q <string>                TrueType query answer given (for papsrv)\n"
msgstr ""
"\t-Q <chaîne>          réponse donnée à une requête TrueType (pour papsrv)\n"

#: ../ppr/ppr_main.c:995
msgid ""
"\t-R copies                  read copy count from document\n"
"\t-R ignore-copies           don't read copy count (default)\n"
msgstr ""
"\t-R copies              lit le nombre de copies dans le document\n"
"\t-R ignore-copies       ne lit le nombre de copies (défaut)\n"

#: ../ppr/ppr_main.c:999
msgid ""
"\t-R duplex:softsimplex      read duplex mode, assume simplex (default)\n"
"\t-R duplex:simplex          read duplex mode, assume simplex\n"
"\t-R duplex:duplex           read duplex mode, assume normal duplex\n"
"\t-R duplex:duplextumble     read duplex mode, assume tumble duplex\n"
"\t-R ignore-duplex           don't read duplex mode\n"
msgstr ""
"\t-R duplex:softsimplex      lit en mode duplex, suppose simplex (défaut)\n"
"\t-R duplex:simplex          lit en mode duplex, suppose simplex\n"
"\t-R duplex:duplex           lit en mode duplex, suppose duplex normal\n"
"\t-R duplex:duplextumble     lit en mode duplex, suppose duplex tumble\n"
"\t-R ignore-duplex           ne lit pas en mode duplex\n"

#: ../ppr/ppr_main.c:987
msgid ""
"\t-R for                     read \"%%For:\" line\n"
"\t-R ignore-for              don't read \"%%For:\" (default)\n"
msgstr ""
"\t-R for                  lit la ligne \"%%For:\"\n"
"\t-R ignore-for           ne lit pas la ligne \"%%For:\" (défaut)\n"

#: ../ppr/ppr_main.c:1006
msgid ""
"\t-R routing                 use routing from \"%%Routing:\" (default)\n"
"\t-R ignore-routing          ignore \"%%Routing:\" comment\n"
msgstr ""
"\t-R routing           utilise le routage à partir de \"%%Routing:\" "
"(défaut)\n"
"\t-R ignore-routing    ignore le commentaire \"%%Routing:\"\n"

#: ../ppr/ppr_main.c:991
msgid ""
"\t-R title                   use title from \"%%Title:\" (default)\n"
"\t-R ignore-title            ignore \"%%Title:\" comment\n"
msgstr ""
"\t-R title                utilise le titre de \"%%Title:\" (défaut)\n"
"\t-R ignore-title         ignore le commentaire \"%%Title:\"\n"

#: ../ppr/ppr_main.c:1048
msgid "\t-U                         unlink job file after queuing it\n"
msgstr ""
"\t-U             efface le fichier de travail après l'avoir mis en queue\n"

#: ../ppop/ppop.c:1086
msgid ""
"\t-X <principal string>\n"
"\t--proxy-for=<principal string>\n"
"\t-M\n"
"\t--machine-readable\n"
"\t--su <user>\n"
"\t-A <seconds>\n"
"\t--arrest-interest-time=<seconds>\n"
"\t--verbose\n"
msgstr ""
"\t-X <chaîne principale>\n"
"\t--proxy-for=<chaîne principale>\n"
"\t-M\n"
"\t--machine-readable\n"
"\t--version\n"
"\t--su <utilisateur>\n"
"\t-A <secondes>\n"
"\t--arrest-interest-time=<secondes>\n"
"\t--verbose\n"

#: ../ppr/ppr_main.c:891
msgid ""
"\t-X <string>                used by network servers to tell PPR the\n"
"\t                           identity of alien users for whom they act\n"
msgstr ""
"\t-X <chaîne>          utilisé par les serveurs réseaux pour indiquer\n"
"\t                     à PPR l'identité des utilisateurs étrangers pour\n"
"\t                     lesquels ils agissent\n"

#: ../ppr/ppr_main.c:1032
msgid "\t-Y                         experimental job splitting feature\n"
msgstr ""
"\t-Y                         possibilité expérimentale de séparation des "
"travaux\n"

#: ../ppr/ppr_main.c:1010
msgid ""
"\t-Z true                    ignore jobs without %%EOF\n"
"\t-Z false                   don't ignore (default)\n"
msgstr ""
"\t-Z true                    ignore les travaux sans %%EOF\n"
"\t-Z false                   ne les ignore pas (défaut)\n"

#: ../ppr/ppr_main.c:931
msgid "\t-a                         turns on authcode mode\n"
msgstr "\t-a                     lance le mode du code d'authentification\n"

#: ../ppr/ppr_main.c:921
msgid ""
"\t-b {yes,no,dontcare}       express banner page preference\n"
"\t-t {yes,no,dontcare}       express trailer page preference\n"
msgstr ""
"\t-b {yes,no,dontcare}       exprime la préférence pour la page de garde "
"avant\n"
"\t-t {yes,no,dontcare}       exprime la préférence pour la page de garde "
"arrière\n"
"\t                           yes = oui, no = non, dontcare = peu importe\n"

#: ../ppr/ppr_main.c:1019
msgid ""
"\t-e none                    don't user stderr or responder for errors\n"
"\t-e stderr                  report errors on stderr (default)\n"
"\t-e responder               report errors by responder\n"
"\t-e both                    report errors with both\n"
"\t-e hexdump                 always use hexdump for no filter\n"
"\t-e no-hexdump              discourage hexdump for no filter\n"
msgstr ""
"\t-e none                    n'utilise pas l'erreur standard ou le répondeur "
"pour les erreurs\n"
"\t-e stderr                  indique les erreurs sur l'erreur standard "
"(défaut)\n"
"\t-e responder               indique les erreurs grâce au répondeur\n"
"\t-e both                    indique les erreurs grâce aux deux\n"
"\t-e hexdump                 utilise toujours hexdump quand pas de filtre\n"
"\t-e no-hexdump              décourage hexdump quand pas de filtre\n"

#: ../ppr/ppr_main.c:907
msgid ""
"\t-m <method>                response method\n"
"\t-m none                    no response\n"
"\t-r <address>               response address\n"
"\t--responder-options <list> list of name=value responder options\n"
msgstr ""
"\t-m <méthode>             méthode de réponse\n"
"\t-m none                  pas de réponse\n"
"\t-r <adresse>             adresse de réponse\n"
"\t--responder-options <liste>  liste d'options nom=valeur pour le répondeur\n"

#: ../ppr/ppr_main.c:983
msgid ""
"\t-n <positive integer>      print n copies\n"
"\t-n collate                 print collated copies\n"
msgstr ""
"\t-n <entier positif>        imprime n copies\n"
"\t-n collate                 imprime des copies dans l'ordre\n"

#: ../ppr/ppr_main.c:974
msgid ""
"\t-o <string>                specify filter options\n"
"\t--markup [format, lp, pr, fallback-lp, fallback-pr]\n"
"\t                           set handling of LaTeX, HTML, and such\n"
msgstr ""
"\t-o <chaîne>                spécifie les options de filtres\n"
"\t--markup [format, lp, pr, fallback-lp, fallback-pr]\n"
"\t                           indique la gestion de LaTeX, HTML et autres\n"

#: ../ppr/ppr_main.c:1045
msgid "\t-q <integer>               sets priority of print job\n"
msgstr ""
"\t-q <entier>             indique la priorité du travail d'impression\n"

#: ../lprsrv/lprsrv.c:161
msgid ""
"\t-s <port>\n"
"\t--standalone-port <port>\n"
"\t\t(run standalone, bind to specified TCP port)\n"
msgstr ""
"\t-s <port>\n"
"\t--standalone-port <port>\n"
"\t\t(tourner en autonome, se lier au port TCP spécifié)\n"

#: ../ppr/ppr_main.c:1027
msgid ""
"\t-s <positive integer>      signature sheet count\n"
"\t-s booklet                 automatic signature sheet count\n"
"\t-s {both,fronts,backs}     indicate part of each signature to print\n"
msgstr ""
"\t-s <entier positif>        compte de la feuille de signature\n"
"\t-s booklet                 compte automatique de la feuille de signature\n"
"\t-s {both,fronts,backs}     indique la partie de chaque signature à "
"imprimer\n"

#: ../ppr/ppr_main.c:925
msgid ""
"\t-w log                     routes warnings to log file\n"
"\t-w stderr                  routes warnings to stderr (default)\n"
"\t-w both                    routes warnings to stderr and log file\n"
"\t-w {severe,peeve,none}     sets warning level\n"
msgstr ""
"\t-w log          dirige les avertissements vers le fichier de log\n"
"\t-w stderr       dirige les avertissements vers l'erreur standard (défaut)\n"
"\t-w both         dirige les avertissements vers le fichier de log et\n"
"\t                l'erreur standard\n"
"\t-w {severe,peeve,none}     indique le niveau d'avertissement\n"

#: ../ppop/ppop_cmds_other.c:1355
msgid ""
"\n"
"\tThis command sets the status of a destination.\n"
"\tThe status of a destination may be displayed with\n"
"\tthe \"ppop destination\" command.\n"
msgstr ""
"\n"
"\tCette commande positionne l'état d'une destination.\n"
"\tL'état d'une destination peut être affiché avec\n"
"\tla commande \"ppop destination\".\n"

#: ../ppop/ppop.c:823
msgid ""
"\n"
"Destination commands:\n"
"\tppop destination {<destination>, all}\n"
"\t\tAbbreviation:  ppop dest {<destination>, all}\n"
"\tppop destination-comment {<destination>, all}\n"
"\t\tAbbreviation:  ppop ldest {<destination>, all}\n"
"\tppop destination-comment-address {<destination>, all}\n"
"\tppop accept <destination>\n"
"\tppop reject <destination>\n"
msgstr ""
"\n"
"Commandes de destination :\n"
"\tppop destination {<destination>, all}\n"
"\t\tabréviation :  ppop dest {<destination>, all}\n"
"\tppop destination-comment {<destination>, all}\n"
"\t\tabréviation :  ppop ldest {<destination>, all}\n"
"\tppop destination-comment-address {<destination>, all}\n"
"\tppop accept <destination>\n"
"\tppop reject <destination>\n"

#: ../ppop/ppop.c:861
msgid ""
"\n"
"Media commands:\n"
"\tppop media {<destination>, all}\n"
"\tppop mount <printer> <bin> <medium>\n"
msgstr ""
"\n"
"Commandes sur les supports :\n"
"\tppop media {<destination>, all}\n"
"\tppop mount <imprimante> <bac> <support>\n"

#: ../ppop/ppop.c:832
msgid ""
"\n"
"Print job commands:\n"
"\tppop list {<destination>, <job>, all} ...\n"
"\tppop short {<destination>, <job>, all} ...\n"
"\tppop details {<destination>, <job>, all} ...\n"
"\tppop lpq {<destination>, <job>, all} [<user>] [<id>] ...\n"
"\tppop qquery {<destination>, <job>, all} <field name 1> ...\n"
"\tppop move {<job>, <old_destination>} <new_destination>\n"
"\tppop hold <job> ...\n"
"\tppop release <job> ...\n"
"\tppop [s]cancel {<job>, <destination>, all} ...\n"
"\tppop [s]purge {<destination>, all} ...\n"
"\tppop [s]cancel-active {<destination>, all} ...\n"
"\tppop [s]cancel-my-active {<destination>, all} ...\n"
"\tppop clean <destination> ...\n"
"\tppop rush <job> ...\n"
"\tppop last <job> ...\n"
"\tppop log <job>\n"
"\tppop progress <job>\n"
"\tppop modify <job> <name>=<value> ...\n"
msgstr ""
