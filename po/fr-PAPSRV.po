#: ../papsrv/papsrv_conf.c:518
#, fuzzy, c-format
msgid "\"%s\" is not a printer or group (line %d)"
msgstr "\"%s\" n'est ni une imprimante ni un groupe (ligne %d)"

#: ../papsrv/papsrv_conf.c:618
#, c-format
msgid ""
"\"AUFSSecurityFor\" must be \"DSC\", \"username\", or \"realname\" (line %d)"
msgstr ""
"\"AUFSSecurityFor\" doit être \"DSC\", \"username\" ou \"realname\" (ligne "
"%d)"

#: ../papsrv/papsrv_conf.c:559
#, c-format
msgid "\"BinaryOK\" option must be \"true\" or \"false\" (line %d)"
msgstr "L'option \"BinaryOK\" doit être \"true\" ou \"false\" (ligne %d)"

#: ../papsrv/papsrv_conf.c:603
#, c-format
msgid "\"ForceAUFSSecurity\" must be \"true\" or \"false\" (line %d)"
msgstr "\"ForceAUFSSecurity\" doit être \"true\" ou \"false\" (ligne %d)"

#: ../papsrv/papsrv_conf.c:605
msgid "\"ForceAUFSSecurity\" option TRUE when no -X switch used"
msgstr "L'option \"ForceAUFSSecurity\" est TRUE quand il n'y a pas d'option -X"

#: ../papsrv/papsrv_conf.c:566
#, c-format
msgid "\"LanguageLevel\" must be a positive integer (line %d)"
msgstr "\"LanguageLevel\" doit être un entier positif (ligne %d)"

#: ../papsrv/papsrv_conf.c:533
#, c-format
msgid "\"PPRname\" must be first line in record (line %d)"
msgstr "\"PPRname\" doit être la première ligne de l'enregistrement (ligne %d)"

#: ../papsrv/papsrv_conf.c:637
#, c-format
msgid "\"QueryFontCache\" option must be \"true\" or \"false\" (line %d)"
msgstr "L'option \"QueryFontCache\" doit être \"true\" ou \"false\" (ligne %d)"

#: ../papsrv/papsrv_conf.c:629
#, c-format
msgid "\"RAMSize\" option must be a positive integer (line %d)"
msgstr "L'option \"RAMSize\" doit être un entier positif (ligne %d)"

#: ../papsrv/papsrv_conf.c:453
#, c-format
msgid "AppleTalk name \"type\" element too long (line %d)"
msgstr "Élément \"type\" du nom AppleTalk trop long (ligne %d)"

#: ../papsrv/papsrv_conf.c:469
#, c-format
msgid "AppleTalk name \"zone\" element too long (line %d)"
msgstr "Élément \"zone\" du nom AppleTalk trop long (ligne %d)"

#: ../papsrv/papsrv_conf.c:442
#, c-format
msgid "AppleTalk name too long (line %d)"
msgstr "Nom AppleTalk trop long (ligne %d)"

#: ../papsrv/papsrv_conf.c:289
#, c-format
msgid "Can't open \"%s\", errno=%d (%s)"
msgstr "Impossible d'ouvrir \"%s\", errno=%d (%s)"

#: ../papsrv/papsrv_conf.c:347
#, c-format
msgid "Can't open configuration file \"%s\", errno=%d (%s)"
msgstr "Impossible d'ouvrir le fichier de configuration \"%s\", errno=%d (%s)"

#: ../papsrv/papsrv_conf.c:493
#, c-format
msgid "Duplicate \"PPRname\" line (line %d)"
msgstr "Ligne \"PPRname\" en double (ligne %d)"

#: ../papsrv/papsrv_conf.c:539
#, c-format
msgid "Duplicate or unnecessary \"PPDFile\" line (line %d)"
msgstr "Ligne \"PPDFile\" en double ou inutile (ligne %d)"

#: ../papsrv/papsrv_conf.c:621
#, c-format
msgid ""
"If the -X switch is not used then \"AUFSSecurityName\" must be \"DSC\" (line "
"%d)"
msgstr ""
"Si l'option -X n'est pas utilisée alors \"AUFSSecurityName\" doit être "
"\"DSC\" (ligne %d)"

#: ../papsrv/papsrv_conf.c:185
#, c-format
msgid "Printer \"%s\" has no \"PPDFile\" line in its config file"
msgstr ""
"L'imprimante \"%s\" n'a pas de ligne \"PPDFile\" dans son fichier de "
"configuration"

#: ../papsrv/papsrv_conf.c:394
msgid "Too many advertised names in config file"
msgstr "Trop de noms annoncés dans le fichier de configuration"

#: ../papsrv/papsrv_conf.c:670
#, c-format
msgid "Unmatched double quote in \"PPRparms\" line (line %d)"
msgstr "Guillemet non fermé dans la ligne \"PPRparms\" (ligne %d)"

#: ../papsrv/papsrv_conf.c:678
#, c-format
msgid "Unmatched single quote in \"PPRparms\" line (line %d)"
msgstr "Apostrophe non fermée dans la ligne \"PPRparms\" (ligne %d)"

#: ../papsrv/papsrv_conf.c:546
#, c-format
msgid "You must put a \"PPDFile\" line here (line %d)"
msgstr "Vous devez mettre une ligne \"PPDFile\" ici (ligne %d)"

#: ../papsrv/papsrv_conf.c:233
#, c-format
msgid "group \"%s\" member \"%s\" does not exist (%s line %d)"
msgstr "Dans le groupe \"%s\", le membre \"%s\" n'existe pas (%s ligne %d)"
